{
  "name": "@ali/g",
  "version": "2.0.0-beta.14",
  "description": "A canvas library which providing 2d draw for G2.",
  "keywords": [
    "canvas",
    "G2",
    "2d draw"
  ],
  "main": "index.js",
  "homepage": "http://gitlab.alibaba-inc.com/datavis/g",
  "author": "https://github.com/orgs/antvis/people",
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:datavis/g.git"
  },
  "bugs": {
    "url": "http://gitlab.alibaba-inc.com/datavis/g/issues"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.1",
    "electron": "^1.7.2",
    "electron-mocha": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-egg": "^4.2.0",
    "is-electron-renderer": "^2.0.1",
    "istanbul": "^0.4.5",
    "pre-commit": "^1.2.2",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shelljs": "^0.7.8",
    "uglify-js": "^3.0.15",
    "spm-sinon": "^1.6.0",
    "event-simulate": "^1.0.0",
    "jquery": "~2.1.4",
    "event-simulate": "^1.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js -- index.js",
    "ci": "tnpm run lint && tnpm run test",
    "compress": "uglifyjs --compress --mangle --output dist/data-set.min.js -- build/data-set.js",
    "coverage": "./bin/coverage.js",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run compress",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./",
    "test": "electron-mocha --renderer --recursive ./test/unit",
    "test-live": "electron-mocha --opts ./test/support/mocha.opts --renderer --interactive --recursive ./test/unit"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "test"
    ],
    "silent": false
  },
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "lodash": "^4.17.4",
    "wolfy87-eventemitter": "^5.1.0",
    "d3-interpolate": "^1.1.5",
    "d3-ease": "^1.0.3"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  }
}
