{
  "name": "@antv/g",
  "version": "2.0.0",
  "description": "A canvas library which providing 2d draw for G2.",
  "keywords": [
    "canvas",
    "G2",
    "2d draw"
  ],
  "main": "index.js",
  "homepage": "http://gitlab.alibaba-inc.com/datavis/g",
  "author": "https://github.com/orgs/antvis/people",
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:datavis/g.git"
  },
  "bugs": {
    "url": "http://gitlab.alibaba-inc.com/datavis/g/issues"
  },
  "devDependencies": {
    "atool-build": "0.6.x",
    "atool-doc": "0.3.0-beta0",
    "atool-test": "~0.4.10",
    "babel-cli": "~6.6.4",
    "babel-core": "~6.7.4",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "dora": "0.3.x",
    "@ali/expect.js": "~0.3.1",
    "jquery": "~2.1.4",
    "less": "~2.6.0",
    "pre-commit": "^1.1.2",
    "eslint": "~1.10.3",
    "eslint-config-airbnb": "~4.0.0",
    "@ali/g-coord": "^1.2.3-beta",
    "spm-sinon": "^1.6.0",
    "event-simulate": "^1.0.0"
  },
  "scripts": {
    "transpiler": "rm -rf lib && babel src --out-dir lib",
    "build": "atool-build",
    "lint": "eslint --ext .js src",
    "test": "atool-test --no-chai",
    "test-keep": "atool-test --no-chai --keep --no-coverage",
    "doc": "atool-doc",
    "doc-build": "atool-doc --build",
    "ci": "tnpm run lint && tnpm run test"
  },
  "entry": {
    "index": "./index"
  },
  "dependencies": {
    "@ali/g-color": "~0.0.7",
    "@ali/g-event": "~0.0.0",
    "@ali/g-interpolation": "~0.0.0",
    "@ali/g-matrix": "~0.0.0",
    "@ali/g-path-util": "~1.0.0",
    "@ali/g-tween": "~0.3.1-beta.1",
    "gl-matrix": "^2.3.2"
  },
  "pre-commit": {
    "run": [
      "test"
    ],
    "silent": false
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  }
}
